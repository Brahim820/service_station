<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">
        <!-- Open matching invoices matching a created record -->
        <record id="open_station_invoices" model="ir.actions.act_window">
            <field name="name">Invoices</field>
            <field name="res_model">account.move</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('source_id', '=', self.id)]</field>
            <field name="context">{}</field>
        </record>
    </data>
</odoo>
